{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "0.0.0",
  "private": true,
  "author": "Mr. Gumby <mr.gumby@example.com>",
  "copyright": "Â© 2017, Gumby inc.",
  "homepage": "http://example.com",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "preunit": "webpack --config=webpack.unit.config.js --env=test --display=errors-only",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "coverage": "npm run unit -- -R scripts/istanbul-reporter",
    "pree2e": "webpack --config=webpack.app.config.js --env=test --display=errors-only && webpack --config=webpack.e2e.config.js --env=test --display=errors-only",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node start.js",
    "release": "webpack --config=webpack.app.config.js --env=production && build"
  },
  "dependencies": {
    "fs-jetpack": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.6.11",
    "electron-builder": "^19.43.3",
    "electron-mocha": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "mocha": "^4.0.1",
    "source-map-support": "^0.5.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
